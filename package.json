    "build": "pnpm build:content && prisma generate && next build",
    "start": "next start",
    "lint": "next lint --dir src",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx,css}'",
    "preinstall": "lefthook install",
    "release": "release-it"
  },
