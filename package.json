    "cheerio": "^1.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.471.1",
    "next": "15.3.2",
    "next-themes": "^0.4.4",
    "openai": "^5.9.0",
    "postcss": "8.5.1",
    "react": "19.1.0",
